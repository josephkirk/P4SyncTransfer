{
  "UseExternalP4": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "P4Sync": "Debug"
    },
    "Console": {
      "FormatterName": "simple",
      "IncludeScopes": false
    }
  },
  "SyncProfiles": [
    {
      "Name": "External P4 Test Sync",
      "Source": {
        "Port": "perforce:1666",
        "User": "testuser",
        "Workspace": "source_workspace"
      },
      "Target": {
        "Port": "perforce:1667",
        "User": "testuser",
        "Workspace": "target_workspace"
      },
      "SyncFilter": [
        "//depot/main/..."
      ]
    }
  ]
}